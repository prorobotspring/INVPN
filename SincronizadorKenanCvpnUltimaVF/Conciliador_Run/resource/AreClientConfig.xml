	<command id="invpn.crear.vpn">
		<parameters>
			<param name="CompanyId" required="true" />
		</parameters>
		<body type="byName">
<![CDATA[
<fc:request xmlns:fc="http://www.digitelcorp.com.ve/integracion/FCFlows" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xmlns="http://www.digitelcorp.com.ve/integracion/Invpn" 
            xsi:schemaLocation="http://www.digitelcorp.com.ve/integracion/FCFlows FCFlows.xsd http://www.digitelcorp.com.ve/integracion/Invpn InvpnRequest.xsd ">
            <fc:header>
                        <fc:TraceId>${TraceId}</fc:TraceId>
                        <fc:target>invpn.crear.vpn</fc:target>
            </fc:header>
            <fc:body>
                        <Plan>${Plan}</Plan>
                        <TraceId>${TraceId}</TraceId>
                        <GroupId>${GroupId}</GroupId>
                        <GroupName>${GroupName}</GroupName>                        
                        <CompanyId>${CompanyId}</CompanyId>
                        <CompanyName>${CompanyName}</CompanyName>
            </fc:body>
</fc:request>
]]>
		</body>
	</command>
	
